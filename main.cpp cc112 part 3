//Header

#ifndef FUNCTIONS_H_INCLUDED
#define FUNCTION_H_INCLUDED
float celToFahren(float celsius)
#endif

// First source 

#include "Functions.h"
float celToFahren(float celsius){
	return (celsius *9/5)+ 32;
}

// Second source 

#include<iostream>
#include"Functions.h"
using namespace std;


int main(){
    float tempC,tempF;
    cout<<"\n Enter a temperature in C: ";
    cin>>tempC;
    tempF =celtofahren(tempC);
    cout<<"\n The temperature in tempF is "<<tempF;

}
